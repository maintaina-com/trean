{
    "name": "horde/trean",
    "description": "Bookmarks application",
    "type": "horde-application",
    "homepage": "https://www.horde.org/apps/trean",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
        },
        {
            "name": "Michael J Rubinsky",
            "email": "mrubinsk@horde.org",
            "role": "lead"
        },
        {
            "name": "Chuck Hagenbuch",
            "email": "chuck@horde.org",
            "role": "lead"
        }
    ],
    "version": "2.0.0",
    "time": "2020-10-27",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com"
        }
    ],
    "require": {
        "horde/horde-installer-plugin": "dev-master",
        "php": "^5.3 || ^7",
        "horde/horde": "dev-master",
        "horde/content": "dev-master",
        "horde/autoloader": "dev-master",
        "horde/controller": "dev-master",
        "horde/core": "dev-master",
        "horde/date": "dev-master",
        "horde/db": "dev-master",
        "horde/exception": "dev-master",
        "horde/form": "dev-master",
        "horde/notification": "dev-master",
        "horde/perms": "dev-master",
        "horde/queue": "dev-master",
        "horde/util": "dev-master",
        "horde/vfs": "dev-master",
        "horde/view": "dev-master",
        "ext-gettext": "*",
        "ext-json": "*"
    },
    "suggest": {
        "horde/browser": "^2",
        "horde/cache": "^2"
    },
    "autoload": {
        "psr-0": {
            "Trean": "lib/"
        }
    },
    "bin": [
        "bin/trean-backfill-crawler",
        "bin/trean-backfill-remove-utm-params",
        "bin/trean-backfill-favicons",
        "bin/trean-url-checker"
    ]
}